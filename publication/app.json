{
  "expo": {
    "name": "StatsCAN",
    "slug": "statcan",
    "version": "3.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#003366"
    },
    "scheme": "stc",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true,
      "bundleIdentifier": "com.statcan.StatCan",
      "buildNumber": "1.0.64",
      "associatedDomains": ["applinks:statcan.gc.ca"]
    },
    "android": {
      "package": "com.statcan.StatCan",
      "versionCode": 64,
      "permissions": [],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "*.statcan.gc.ca",
              "pathPrefix": "/article"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
