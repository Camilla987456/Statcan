{
  "cli": {
    "version": ">= 0.55.1"
  },
  "build": {
    "development": {
      "developmentClient": true
    },
    "alpha": {
      "env": {
        "APP_VARIANT": "alpha"
      }
    },
    "production": {
      "env": {
        "APP_VARIANT": "production"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "internal",
        "releaseStatus": "draft"
      }
    },
    "alpha": {
      "android": {
        "track": "internal",
        "releaseStatus": "draft"
      }
    }
  }
}
